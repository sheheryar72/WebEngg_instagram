    <script>
    window.addEventListener('load', function () {
        $.connection.hub.start().done(onHubStart).fail(failed);

        function onHubStart() {
            commentshub.client.newMessage = new function (Post_ID, Username, Comment) {
                alert('new message function called');
                init();
                //console.log({ Post_ID, Username, Comment });
                console.log("Hub Start");
            }
             function commentidgrabber(commentgrab) {
                var Post_ID = commentgrab.replace('comment', '')
                var commenttextss = getElementById('commenttext' + Post_ID).value;
                save_comment(Post_ID, commenttextss);
            }
            function save_comment(postid, comment) {
                const CurrentUsername = @Session["user"].ToString();
                const CurrentPostID = postid;
                const CurrentComment = comment;
                commentshub.hub.server.comment(CurrentPostID, CurrentUsername, CurrentComment);
            }
        }
        function failed() {
            console.log("failed");
        }
    }, 5000);
    </script>



